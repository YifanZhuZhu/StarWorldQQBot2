{
  "extends": "ts-node/node16/tsconfig.json",
  "ts-node": {
    "transpileOnly": true,
    "files": true,
    "moduleTypes": {},
    "compilerOptions": {
      "lib": [
        "esnext",
        "dom"
      ]
    }
  },
  "compilerOptions": {
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "swbot": ["./src/"],
      "swbot/": ["./src"],
      "swbot/*": ["./src/*"],
      "@/*": ["./src/*"],
      "~/*": ["./*"]
    }
  }
}
